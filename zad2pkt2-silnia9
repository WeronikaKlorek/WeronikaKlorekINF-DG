using System;

namespace Zadanie2_1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int  silnia = 1;
            int liczba = 9

            for (int i = 1; i <= liczba; i++)
            {

                silnia *= i;

            }

            Console.WriteLine(silnia);


            Console.ReadKey();
        }
    }
}
